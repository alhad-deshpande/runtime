<Project Sdk="Microsoft.NET.Sdk.IL">
  <PropertyGroup>
  <!-- Needed for CLRTestTargetUnsupported -->
  <RequiresProcessIsolation>true</RequiresProcessIsolation>
  <!-- Interpreter-only stack overflow -->
  <CLRTestTargetUnsupported Condition="'$(TargetArchitecture)' == 'ppc64le'">true</CLRTestTargetUnsupported>
  <PropertyGroup>
    <DebugType>Full</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="reference_i_d.il" />
    <Compile Include="reference_i.il" />
  </ItemGroup>
</Project>
